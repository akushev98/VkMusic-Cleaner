<Window x:Class="Delete_Dublicate.AuthorizationForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Delete_Dublicate"
        mc:Ignorable="d"
        Title="AuthorizationForm" Height="400" Width="700"
        Loaded="AuthorizationForm_Load">
    <Grid Margin="0"
          VerticalAlignment="Stretch" 
            HorizontalAlignment="Stretch" >
        <WebBrowser 
            VerticalAlignment="Stretch" 
            HorizontalAlignment="Stretch" 
            Name="GetToken" 
            Margin="0" 
            RenderTransformOrigin="0.5,0.5"
            LoadCompleted="GetToken_DocumentCompleted">
            <WebBrowser.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <TranslateTransform/>
                </TransformGroup>                
            </WebBrowser.RenderTransform>            
        </WebBrowser>
    </Grid>
</Window>
